<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.3.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.3.0/distro-source/core/src/gwt-module.dtd">
<module rename-to='cas'>
	<!-- Inherit the core Web Toolkit stuff. -->

	<inherits name='com.google.gwt.user.User' />
	<inherits name="com.google.gwt.uibinder.UiBinder" />
	<inherits name="com.google.gwt.inject.Inject" />

	<inherits name='com.google.gwt.resources.Resources' />
	<inherits name='com.gwtplatform.dispatch.Dispatch' />
	<inherits name="com.gwtplatform.mvp.Mvp" />


	<!-- Inherit the default GWT style sheet. You can change -->
	<!-- the theme of your GWT application by uncommenting -->
	<!-- any one of the following lines. -->
	<inherits name='com.google.gwt.user.theme.standard.Standard' />
	<!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
	<!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/> -->

	<inherits name='ca.uSherbrooke.gegi.persist.cas.Cas' />



	<!-- Specify the app entry point class.
	<entry-point class='ca.uSherbrooke.gegi.ui.cas.client.Cas' />   -->

	<!-- Specify the paths for translatable code -->
	<source path="client" />
	<source path='shared' />

	<!-- This is required for Gwt-Platform Proxy's generator 
	<define-configuration-property name="gin.ginjector" is-multi-valued="false" /> -->
<!--	<set-configuration-property name="gin.ginjector" value="ca.uSherbrooke.gegi.ui.cas.client.gin.ClientGinjector" /> -->
	<set-configuration-property name="UiBinder.useSafeHtmlTemplates" value="true" />
</module>